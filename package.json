{
  "name": "ChatRoomDemo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "rn-android": "react-native run-android",
    "rn-ios": "react-native run-ios",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "rn-start": "react-native start",
    "test": "jest",
    "start": "expo start --dev-client",
    "startr": "expo start --dev-client --clear",
    "startr8888": "expo start --dev-client --clear --port 8888",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "env": "node ./generate-env.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "expo": "~48.0.21",
    "expo-av": "~13.2.1",
    "expo-dev-client": "~2.2.1",
    "expo-font": "~11.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.14",
    "react-native-chat-room": "git+https://github.com/AsteriskZuo/react-native-chat-room.git#019073e645b167812548dc738b6d8aca7f4e1074",
    "react-native-chat-sdk": "1.3.0-beta.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "twemoji": "^14.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/config-conventional": "17.7.0",
    "@evilmartians/lefthook": "^1.5.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/jest": "^29.2.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "0.70.0",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "commitlint": "17.7.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "expo-doctor": "^1.1.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.10",
    "prettier": "^2.0.5",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.9.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "resolutions": {
    "@expo/cli": "*",
    "@types/react": "~17.0.21",
    "expo/expo-asset": "8.9.1"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "rules": {
      "jest/*": 0,
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error",
      "react-native/no-inline-styles": 0,
      "@typescript-eslint/no-shadow": "off",
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test",
          "tag",
          "example"
        ]
      ]
    }
  }
}
